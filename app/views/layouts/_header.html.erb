<div class="page-header">
	<h1>タスク管理アプリ</h1>
  <% if current_user %>
    <%= link_to 'ユーザー情報', user_path(current_user.id) %>
    <%= link_to 'ログアウト', sessions_path, method: :delete %>
  <% else %>
    <%= link_to 'ログイン', new_session_path %>
    <%= link_to '新規登録', new_user_path %>
  <% end %>
</div>