<h1>Sessions#new</h1>
<%= form_with( scope: :sessions, local: true) do |form| %>
  <div class="form-group">
    <%= form.label :email %>
    <%= form.text_field :email, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :password %>
    <%= form.password_field :password, class: "form-control" %>
  </div>
  <%= form.submit %>
<% end %>